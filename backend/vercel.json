{
    "version": 2,
    "builds": [
      {
        "src": "server.js", 
        "use": "@vercel/node"
      }
    ],
  
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
             "Access-Control-Allow-Origin": "*",
             "Access-Control-Allow-Methods": ["OPTIONS", "POST", "GET","PUT", "DELETE", "PATCH"],
             "Access-Control-Allow-Headers":"*"
         }
      }
    ] 
    
  }